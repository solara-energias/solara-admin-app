{
  "package": {
    "productName": "Solara Admin",
    "version": "0.1.0"
  },
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn dev",
    "devPath": "http://localhost:3000",
    "distDir": "../out"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "solara-admin-app",
      "publisher": "bluey",
      "copyright": "",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null,
        "providerShortName": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "language": "pt-BR"
        }
      }
    },
    "updater": {
      "active": true,
      "dialog": false,
      "endpoints": [
        "https://releases.bluey.tech/tauri/{{target}}/{{arch}}/{{current_version}}"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEY0NjFFNjQxNUY5RkI3NTAKUldSUXQ1OWZRZVpoOUE4OVBXWFBPTG5GdUt1TDdXRGdBRWNKMlJTTmEvNUcxb1FNdTUrcHpDRFEK"
    },
    "allowlist": {
      "all": false,
      "http": {
        "all": false,
        "request": true,
        "scope": ["http://solaraenergias.net.br/*"]
      },
      "clipboard": {
        "all": false,
        "readText": false,
        "writeText": true
      }
    },
    "windows": [
      {
        "title": "Solara Admin App",
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "maximizable": true
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}
